{
  "name": "Design-Tokens",
  "version": "0.6.0",
  "description": "Your Figma Plugin",
  "main": "code.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/lukasoppermann/design-tokens.git"
  },
  "scripts": {
    "start": "npm run build-watch",
    "build": "tsc-bundle tsconfig.json",
    "build-watch": "npm run test && npm run build -- --watch",
    "test": "tsc src/index.ts --target es6 --noEmit --typeRoots \"./node_modules/@types,./node_modules/@figma\"",
    "amazn": "style-dictionary build --config ./tests/amazn-style-dictionary/config.json"
  },
  "author": "Lukas Oppermann <oppermann.lukas@gmail.com> (https://vea.re)",
  "license": "MIT",
  "devDependencies": {
    "@figma/plugin-typings": "^1.16.1",
    "install": "^0.13.0",
    "npm": "^6.14.8",
    "style-dictionary": "^2.10.1",
    "typescript-bundle": "^1.0.16"
  },
  "dependencies": {
    "@types/node": "^14.11.2",
    "typescript": "^4.0.3"
  }
}
